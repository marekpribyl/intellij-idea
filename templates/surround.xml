<templateSet group="surround">
  <template name="equa" value="assertEquals($CURSOR$, $SELECTION$);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="DATE" value="new Date($SELECTION$.getTime())" description="Surround with new Date()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="FMT" value="String.format($SELECTION$, $CURSOR$)" description="String.format()" toReformat="false" toShortenFQNames="true">
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="PP" value="!($SELECTION$)" description="Surround with !()" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="KOTLIN" value="true" />
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>