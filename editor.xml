<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="1000" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="bfa222" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="b2b2b2" />
    </pattern>
    <pattern icon="important" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="bfa222" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="b2b2b2" />
    </pattern>
    <pattern icon="question" useCustomColors="false" case-sensitive="false" pattern="\bxxx\b.*">
      <option name="FOREGROUND" value="bfa222" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="b2b2b2" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="false" pattern="\bfixme\b tests.*">
      <option name="FOREGROUND" value="bfa222" />
      <option name="BACKGROUND" value="ff6b6f" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="b2b2b2" />
    </pattern>
    <filter name="FIXME">
      <pattern index="1" />
    </filter>
    <filter name="FIXME tests">
      <pattern index="3" />
    </filter>
  </component>
</application>